{
	"useTabs": true,
	"singleQuote": true,
	"trailingComma": "none",
	"semi": true,
	"printWidth": 100,
	"endOfLine": "auto",
	"importOrder": [
		"^(^react$|@react|react|^recoil$)",
		"^@mui/(.*)$",
		"<THIRD_PARTY_MODULES>",
		"^@/(.*)$",
		"^[./]"
	],
	"importOrderGroupNamespaceSpecifiers": true,
	"importOrderSeparation": true,
	"importOrderSortSpecifiers": true
}
